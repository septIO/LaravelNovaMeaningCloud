{
    "name": "septio/laravel-nova-meaning-cloud",
    "description": "A field for Laravel Nova that generates summarized text from another text field by using Meaning Cloud's API.",
    "keywords": [
        "laravel",
        "nova",
        "summarization"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Septio\\LaravelMeaningcloud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Septio\\LaravelMeaningcloud\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
